              "id": "anthropic-llm-example",
              "name": "Proxy LLM request - Anthropic Claude",
              "request": {
                "name": "Proxy LLM request - Anthropic Claude",
                "description": {
                  "content": "Example LLM request using Anthropic Claude 3 Sonnet via ReliAPI. Includes idempotency and caching.",
                  "type": "text/plain"
                },
                "url": {
                  "path": [
                    "proxy",
                    "llm"
                  ],
                  "host": [
                    "{{baseUrl}}"
                  ]
                },
                "header": [
                  {
                    "key": "Content-Type",
                    "value": "application/json"
                  }
                ],
                "method": "POST",
                "body": {
                  "mode": "raw",
                  "raw": "{\n  \"target\": \"anthropic\",\n  \"messages\": [\n    {\n      \"role\": \"user\",\n      \"content\": \"Explain what idempotency means in API design.\"\n    }\n  ],\n  \"model\": \"claude-3-sonnet-20240229\",\n  \"max_tokens\": 200,\n  \"temperature\": 0.7,\n  \"idempotency_key\": \"anthropic-{{$randomUUID}}\",\n  \"cache\": 3600\n}",
                  "options": {
                    "raw": {
                      "language": "json"
                    }
                  }
                }
              },
              "event": [
                {
                  "listen": "prerequest",
                  "script": {
                    "type": "text/javascript",
