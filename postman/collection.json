              "name": "Proxy HTTP request",
              "request": {
                "name": "Proxy HTTP request",
                "description": {
                  "content": "Universal HTTP proxy endpoint for any HTTP API. Supports retries, circuit breaker, cache, and idempotency. Use this endpoint to add reliability layers to any HTTP API call.\n\nExample: Proxying a GET request to JSONPlaceholder API with caching.",
                  "type": "text/plain"
                },
                "url": {
                  "path": [
                    "proxy",
                    "http"
                  ],
                  "host": [
                    "{{baseUrl}}"
                  ],
                  "query": [],
                  "variable": []
                },
                "header": [
                  {
                    "key": "Content-Type",
                    "value": "application/json"
                  },
                  {
                    "key": "Accept",
                    "value": "application/json"
                  },
                  {
                    "key": "X-RapidAPI-Key",
                    "value": "{{rapidApiKey}}",
                    "type": "text",
                    "disabled": true
                  },
                  {
                    "key": "Authorization",
                    "value": "Bearer {{apiKey}}",
                    "type": "text",
                    "disabled": true
                  }
                ],
                "method": "POST",
                "body": {
                  "mode": "raw",
                  "raw": "{\n  \"target\": \"jsonplaceholder\",\n  \"method\": \"GET\",\n  \"path\": \"/posts/1\",\n  \"headers\": {},\n  \"query\": {},\n  \"idempotency_key\": \"http-{{$randomUUID}}\",\n  \"cache\": 300\n}",
                  "options": {
                    "raw": {
                      "headerFamily": "json",
                      "language": "json"
                    }
                  }
                },
                "auth": null
              },
              "event": [
                {
                  "listen": "prerequest",
                  "script": {
                    "type": "text/javascript",
                    "exec": [
                      "// Auto-set authorization header based on environment",
                      "if (pm.environment.get('rapidApiKey')) {",
                      "    pm.request.headers.add({",
                      "        key: 'X-RapidAPI-Key',",
                      "        value: pm.environment.get('rapidApiKey')",
                      "    });",
                      "} else if (pm.environment.get('apiKey')) {",
                      "    pm.request.headers.add({",
                      "        key: 'Authorization',",
                      "        value: 'Bearer ' + pm.environment.get('apiKey')",
                      "    });",
                      "}"
                    ]
                  }
                },
                {
                  "listen": "test",
                  "script": {
                    "type": "text/javascript",
                    "exec": [
                      "pm.test('Status code is 200', function () {",
                      "    pm.response.to.have.status(200);",
                      "});",
                      "",
                      "pm.test('Response has JSON body', function () {",
                      "    pm.response.to.be.json;",
                      "});",
                      "",
                      "pm.test('Response contains cache information', function () {",
                      "    const headers = pm.response.headers.toObject();",
                      "    pm.expect(headers).to.have.property('x-cache-hit');",
                      "});",
                      "",
                      "pm.test('Response time is reasonable', function () {",
                      "    pm.expect(pm.response.responseTime).to.be.below(10000);",
                      "});"
                    ]
